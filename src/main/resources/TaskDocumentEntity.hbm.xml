<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.demo.entity.TaskDocumentEntity" table="task_document" schema="hiqc">
        <id name="id" column="id"/>
        <property name="documentId" column="document_id"/>
        <property name="taskId" column="task_id"/>
        <property name="fileName" column="file_name"/>
    </class>
</hibernate-mapping>