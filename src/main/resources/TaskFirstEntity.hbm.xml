<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.demo.entity.TaskFirstEntity" table="task_first" schema="hiqc">
        <id name="id" column="id"/>
        <property name="taskDocumentId" column="task_document_id"/>
        <property name="testDocumentNum" column="test_document_num"/>
        <property name="correctDocumentNum" column="correct_document_num"/>
        <property name="errorDocumentNum" column="error_document_num"/>
        <property name="score" column="score"/>
        <property name="contentFields" column="content_fields"/>
        <property name="missingFields" column="missing_fields"/>
        <property name="documentText" column="document_text"/>
    </class>
</hibernate-mapping>