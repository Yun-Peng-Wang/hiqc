<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.demo.entity.TaskRecordSecondEntity" table="task_record_second" schema="hiqc">
        <id name="id" column="id"/>
        <property name="taskDocumentId" column="task_document_id"/>
        <property name="path" column="path"/>
        <property name="rename" column="rename"/>
        <property name="uploadTime" column="upload_time"/>
        <property name="size" column="size"/>
    </class>
</hibernate-mapping>